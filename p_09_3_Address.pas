var
    city, street, home, apt : string;
begin
    writeln('City?');
    readln(city);
    writeln('Street?');
    readln(street);
    writeln('Home?');
    readln(home);
    writeln('Apartment?');
    readln(apt);
    writeln('City: ' + city + ' Street: ' + street + ' Home: ' + home + 
        ' Apartment: ' + apt);
end.
